class Solution {
    public int maximumWealth(int[][] accounts) {
        int max = Integer.MIN_VALUE;
        
        for(int[] acc: accounts ){// Itreate over rows, and take the sum in innt sum
            int sum= 0;
            for(int person:acc){
                sum+=person; // sum of the rows

            }
            if(sum>max){
                max=sum;
            }
            
        }
        return max;

        
    }
}
